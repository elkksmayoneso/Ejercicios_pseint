Algoritmo costo_impresion
	
	Definir paginas, precioPagina, total Como Real
    
    Escribir "Ingrese el número de páginas: "
    Leer paginas
    Escribir "Ingrese el precio por página: "
    Leer precioPagina
    
    total <- paginas * precioPagina
    
    Escribir "El costo total de impresión es: ", total
	
FinAlgoritmo
